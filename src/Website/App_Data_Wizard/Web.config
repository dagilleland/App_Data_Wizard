<?xml version="1.0"?>
<configuration>
    <appSettings>
        <add key="SqlScriptFolder"
             value="~/App_Data/Scripts/Sql/"/>
        <!--Script Naming Pattern. E.g.: 01.00.00.school.sql-->
        <add key="SqlScriptNamingPattern"
             value="\d{1,2}\.\d{1,2}\.\d{1,2}\..*.sql"/>
        <add key="SqlScriptLastExecuted"
             value=""/>
    </appSettings>
</configuration>
